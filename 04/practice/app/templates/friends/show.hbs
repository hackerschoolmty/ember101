<div class="friend-profile">
  <p>{{model.firstName}}</p>
  <p>{{model.lastName}}</p>
  <p>{{model.email}}</p>
  <p>{{model.twitter}}</p>
  <p>{{link-to "Edit info" "friends.edit" model}}</p>
  <p><button {{action "delete" model}}>Delete</button></p>
  <p>{{link-to "Back to friends" "friends.index"}}</p>
  <p>{{link-to "Lend article" "articles.new"}}</p>
</div>

<div class="articles-container">
  <h3>Borrowed Articles</h3>
  {{outlet}}
</div>
